{
  "name": "@ouroboros/feeder",
  "private": "true",
  "description": "HonoJs + temporalio application that orchestras the services.",
  "scripts": {
    "build": "tsc --build",
    "dev": "dotenv -e .env tsx watch src/index.ts",
    "lint": "eslint .",
    "start:worker": "dotenv -e .env tsx watch src/domains/temporal/worker.ts",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/s3-request-presigner": "^3.583.0",
    "@hono/node-server": "^1.11.1",
    "@ouroboros/s3-client": "workspace:*",
    "@ouroboros/weather-schema": "workspace:*",
    "@ouroboros/weathercore-representations": "workspace:*",
    "@temporalio/activity": "^1.9.0",
    "@temporalio/client": "^1.9.0",
    "@temporalio/worker": "^1.9.0",
    "@temporalio/workflow": "^1.9.0",
    "dotenv-cli": "^7.4.2",
    "graphql": "^16.8.1",
    "graphql-request": "^7.0.1",
    "hono": "^4.3.11",
    "nanoid": "3.x",
    "node-fetch": "^3.3.2",
    "zod": "^3.23.8",
    "zod-fetch": "^0.1.1"
  },
  "devDependencies": {
    "@ouroboros/eslint-config": "workspace:*",
    "@ouroboros/typescript-config": "workspace:*",
    "@types/node": "^20.11.17",
    "@types/node-fetch": "^2.6.11",
    "@vitest/coverage-v8": "^1.6.0",
    "ts-to-zod": "^3.8.5",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
