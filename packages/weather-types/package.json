{
  "name": "@ouroboros/weather-schema",
  "version": "1.0.0",
  "description": "weather-schema generates JSON-Schema and exports reusable Zod Schema for runtime validation, servicing the development of a type-safe external-facing API",
  "exports": {
    ".": "./src/index.ts"
  },
  "main": "./src/index.ts",
  "scripts": {
    "build": "tsc --build",
    "generate:schema": "tsx ./src/utils/generate-schema.ts",
    "generate:zod": "tsx ./src/utils/generate-zod.ts"
  },
  "keywords": [],
  "author": "Syahrul ANuar",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@ouroboros/eslint-config": "workspace:*",
    "@ouroboros/typescript-config": "workspace:*",
    "@types/node": "^20.12.12",
    "genson-js": "^0.0.8",
    "json-schema-to-zod": "^2.1.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "zod": "^3.23.8"
  }
}
